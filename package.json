{
  "name": "typescript-node-starter",
  "version": "1.0.0",
  "description": "Typescript",
  "scripts": {
    "start": "ts-node-dev --respawn src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "eslint-check": "npx eslint src/index.ts",
    "eslint-fix": "npx eslint src/index.ts --fix",
    "prettier-start": "npx prettier --single-quote --tab-width 2 src/index.ts",
    "prettier-clean": "npx prettier src/index.ts",
    "prettier-write": "prettierrc --write src/index.ts",
    "test:unit": "mocha"
  },
  "keywords": [
    "fp-ts",
    "typescript",
    "starter",
    "template"
  ],
  "author": "Oscar Prabhakar https://github.com/oscarprabhakar",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "10.0.6",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-joyn": "4.2.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-functional": "6.5.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-mocha": "10.4.3",
    "eslint-plugin-prettier": "^5.1.3",
    "fishery": "2.2.2",
    "mocha": "10.4.0",
    "nodemon": "3.1.1",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarprabhajar/ts-template.git"
  },
  "bugs": {
    "url": "https://github.com/oscarprabhakar/ts-template/issues"
  },
  "homepage": "https://github.com/oscarprabhakar/ts-template#readme",
  "dependencies": {
    "fp-ts": "2.16.6"
  }
}
