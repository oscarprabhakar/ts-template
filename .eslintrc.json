{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "eslint-plugin-prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
